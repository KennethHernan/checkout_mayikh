{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api"
    },
    {
      "src": "/api/create_preference",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/index.js"
    }
  ]
}